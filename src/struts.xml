<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="default" namespace="/" extends="struts-default, json-default">
		<action name="login" class="com.TMMS.Main.action.LoginAction">
			<result name="success">
				/JSP/index.jsp
			</result>
			<result name="error">
				/index.jsp
			</result>
		</action>
		<action name="logout" class="com.TMMS.Main.action.LogoutAction">
			<result name="success">
				/index.jsp
			</result>
			<result name="error">
				/JSP/Error.jsp
			</result>
		</action>
		<action name="showUserInformation" class="com.TMMS.Main.action.ShowUserInformationAction">
			<result name="success">
				/JSP/HTML/tab1/show_user_information.jsp
			</result>
			<result name="error">
				/JSP/Error.jsp
			</result>
		</action>
		<action name="fixUserInformation" class="com.TMMS.Main.action.FixUserInformationAction">
			<result name="success">
				/JSP/HTML/tab1/fix_user_information.jsp
			</result>
			<result name="error">
				/JSP/Error.jsp
			</result>
		</action>
		<action name="fixUserInformationDown" class="com.TMMS.Main.action.FixUserInformationActionDown">
			<result name="success">
				/JSP/Success.jsp
			</result>
			<result name="error">
				/JSP/Error.jsp
			</result>
		</action>
		<action name="fixUserPassword" class="com.TMMS.Main.action.FixUserPasswordAction">
			<result name="success">
				/JSP/Success.jsp
			</result>
			<result name="error">
				/JSP/Error.jsp
			</result>
		</action>
		<action name="showUserLoginLog" class="com.TMMS.Main.action.ShowUserLoginLogAction">
			<result name="success">
				/JSP/HTML/tab1/show_user_loginLog.jsp
			</result>
			<result name="error">
				/JSP/Error.jsp
			</result>
		</action>
		<action name="showHomeInformation" class="com.TMMS.Main.action.HomeInformationAction">
			<result name="success">
				/JSP/HTML/Home.jsp
			</result>
			<result name="error">
				/JSP/Error.jsp
			</result>
		</action>
	</package>
</struts>